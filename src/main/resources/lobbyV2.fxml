         <TextArea fx:id="_fieldChatLobby" editable="false" layoutX="33.0" layoutY="451.0" prefHeight="82.0" prefWidth="559.0" wrapText="true" />
         <TextArea fx:id="_fieldChatGlobal" layoutX="323.0" layoutY="56.0" prefHeight="189.0" prefWidth="415.0" />
         <Button fx:id="_btnRejoindre" layoutX="671.0" layoutY="275.0" mnemonicParsing="false" onAction="#joindreSalon" text="Rejoindre" />
         <VBox fx:id="_listeSalon" layoutX="33.0" layoutY="47.0" onMouseClicked="#selectionSalon" prefHeight="386.0" prefWidth="174.0" />
